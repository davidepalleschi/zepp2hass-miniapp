{
  "configVersion": "v3",
  "app": {
    "appId": 25977,
    "appName": "Zepp2Hass",
    "appType": "app",
    "version": {
      "code": 1,
      "name": "1.1.0"
    },
    "icon": "icon.png",
    "vender": "davide",
      "description": "A ZeppOS watch app that collects health and fitness metrics from your smartwatch and sends them to a custom HTTP endpoint."
  },
  "permissions": [
    "device:os.local_storage",
    "data:user.hd.heart_rate",
    "data:user.hd.spo2",
    "data:user.hd.calorie",
    "data:user.hd.distance",
    "data:user.hd.fat_burning",
    "data:user.hd.pai",
    "data:user.hd.stand",
    "data:user.hd.step",
    "data:user.hd.stress",
    "data:user.hd.sleep",
    "data:user.hd.body_temp",
    "data:user.hd.workout",
    "data:user.info",

    "data:os.device.info",
    "device:os.alarm",
    "device:os.bg_service",
    "device:os.notification",
    "system:network.internet",
    "device:os.ble"
  ],
  "runtime": {
    "apiVersion": {
      "compatible": "4.0.0",
      "target": "4.0.0",
      "minVersion": "4.0"
    }
  },
  "targets": {
    "common": {
      "module": {
        "page": {
          "pages": [
            "page/index"
          ]
        },
        "app-side": {
          "path": "app-side/index"
        },
        "setting": {
          "path": "settings/index"
    },
        "app-service": {
          "services": ["app-service/background_service"]
        }
      },
      "platforms": [
        {
          "st": "r",
          "dw": 480
        },
        {
          "st": "s",
          "dw": 480
        }
      ]
    }
  },
  "defaultLanguage": "en-US"
}
